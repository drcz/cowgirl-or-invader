;;; tail-recursive version:

((PROC (NAME n NAME a CONST 0 LOOKUP n EQ SELECT (LOOKUP a FORGET a FORGET n) (LOOKUP a LOOKUP n MUL CONST 1 LOOKUP n SUB LOOKUP f0 FORGET a FORGET n APPLY)) NAME f0
